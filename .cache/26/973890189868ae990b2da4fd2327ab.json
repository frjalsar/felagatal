class=\"col-sm-9\">\n              <input\n                id=\"club.abbreviation\"\n                v-model=\"club.abbreviation\"\n                type=\"text\"\n                class=\"form-control\"\n                :disabled=\"working\"\n              >\n            </div>\n          </div>\n          <div class=\"form-group row\">\n            <label\n              for=\"club.country\"\n              class=\"col-sm-3 col-form-label text-right\"\n            >Íþróttahérað:</label>\n            <div class=\"col-sm-9\">\n              <select\n                id=\"club.country\"\n                v-model=\"club.provinceId\"\n                class=\"form-control\"\n                :disabled=\"working\"\n              >\n                <option\n                  v-for=\"province in provinces\"\n                  :key=\"province.id\"\n                  :value=\"province.id\"\n                >\n                  {{ province.fullname }}\n                </option>\n              </select>\n            </div>\n          </div>\n          <div\n            v-if=\"club.thorId\"\n            class=\"form-group row\"\n          >\n            <label\n              for=\"club.id\"\n              class=\"col-sm-3 col-form-label text-right\"\n            >Fiffó kóði:</label>\n            <div class=\"col-sm-9\">\n              <input\n                id=\"club.id\"\n                v-model=\"club.thorId\"\n                type=\"text\"\n                readonly\n                class=\"form-control-plaintext\"\n                :disabled=\"working\"\n              >\n            </div>\n          </div>\n          <button\n            type=\"submit\"\n            class=\"btn btn-secondary mt-4\"\n            :disabled=\"working\"\n            @click.prevent=\"save\"\n          >\n            Vista\n          </button>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport agent from 'superagent'\n\nexport default {\n  name: 'Felag',\n  data () {\n    return {\n      working: false,\n      updated: null,\n      club: {},\n      provinces: []\n\n    }\n  },\n  mounted () {\n    agent\n      .get(process.env.VUE_APP_API_HOST + '/clubs/' + this.$route.params.id)\n      .then(res => {\n        if (res.body[0]) {\n          this.club = res.body[0]\n        } else {\n          this.club = {\n            id: 0,\n            fullName: '',\n            shortName: '',\n            abbreviation: '',\n            provinceId: 0,\n            thorId: null\n          }\n        }\n      })\n\n    agent\n      .get(process.env.VUE_APP_API_HOST + '/provinces/')\n      .then(res => {\n        this.provinces = res.body\n      })\n  },\n  methods: {\n    save () {\n      this.working = true\n      const method = this.club.id ? 'put' : 'post'\n      const path = process.env.VUE_APP_API_HOST + '/clubs'\n      return agent(method, path)\n        .send(this.club)\n        .then(res => {\n          this.updated = !!res.body.id\n          this.working = false\n          setTimeout(() => {\n            this.updated = false\n          }, 2000)\n        })\n    }\n  }\n}\n</script>\n\n<style>\nh1 i {\n  cursor: pointer;\n  padding-right: 10px\n}\n\n.fa-check-circle {\n  color: #ccc;\n  font-size: 20px;\n}\n\n.fa-check-circle.verified {\n  color: #c3e6cb\n}\n\n.fade-enter-active, .fade-leave-active {\n  transition: opacity .5s;\n}\n.fade-enter, .fade-leave-to /* .fade-leave-active below version 2.1.8 */ {\n  opacity: 0;\n}\n</style>\n"},"lineCount":null}},"error":null,"hash":"4ad2cf1fd8bcf6c4cc8087b53913086e","cacheData":{"env":{}}}